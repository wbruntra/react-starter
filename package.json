{
  "name": "newest-react-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon --inspect ./bin/www",
    "dev": "concurrently \"npm run server\" \"npm run start --prefix client\"",
    "prod:test": "NODE_ENV=production npm start"
  },
  "dependencies": {
    "concurrently": "^5.0.2",
    "cookie-parser": "~1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "~4.1.1",
    "express": "~4.17.1",
    "http-errors": "~1.7.3",
    "husky": "^4.2.5",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1"
  },
  "devDependencies": {
    "pretty-quick": "^2.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
